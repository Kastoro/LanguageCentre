#following
  #c_form_follow
    a.close(href="#club-close")
      img(src="images/png/dash.png")
    h5.cform_header Подписаться<br>на e-mail рассылку
    div
      .wrap-form.name
        input.c-form(type='text' value='' name='name' placeholder='Ваше имя')
        p.asterisk *
      .wrap-form
        input.c-form(class="c-form" type="email" value="" name='email' placeholder='Ваш e-mail')

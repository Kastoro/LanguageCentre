//- заказать звонок
#order_call
  #c_form_new
    a.close(href="#")
      img(src="images/png/dash.png")
    h5.cform_header Заказать<br>обратный звонок
    div
      .wrap-form.name
        input.c-form(type='text' value='' name='name' placeholder='Ваше имя')
        p.asterisk *
      .wrap-form.name
        input.c-form(type='text' value='' name='name' placeholder='Ваша фамилия')
        p.asterisk *
      .wrap-form
        input.c-form(type='tel' value='' name='phone' placeholder='Ваш номер телефона')
        p.asterisk *
      .btn
        a.bttn-yellow(href='#callback-15-min') Заказать звонок
